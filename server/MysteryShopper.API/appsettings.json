{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
        }
    },
    "AllowedHosts": "*",
    "DATABASE_URL": "DATABASE_URL",
    "Jwt": {
        "Issuer": "JWT_ISSUER",
        "Audiences": ["JWT_AUDIENCE_!", "JWT_AUDIENCE_2"],
        "AccessSecretKey": "JWT_ACCESS_KEY",
        "RefreshSecretKey": "JWT_REFRESH_KEY",
        "AccessMinutesExpire": 123,
        "RefreshDaysExpire": 123
    },
    "Email": {
        "From": "EMAIL_FROM",
        "Address": "EMAIL_ADDRESS",
        "Password": "EMAIL_PASSWORD"
    },
    "Cors": {
        "Origins": ["CORS_ORIGIN_1"]
    },
    "MistralAPI": {
        "Key": "MISTRAL_API_KEY",
        "Uri": "MISTRAL_API_URI"
    },
    "Minio": {
        "Endpoint": "MINIO_ENDPOINT",
        "AccessKey": "MINIO_ACESS_KEY",
        "SecretKey": "MINIO_SECRET_KEY",
        "BucketNames": {
            "User": "MINIO_USER_BUCKET_NAME",
            "Company": "MINIO_COMPANY_BUCKET_NAME",
            "Report": "MINIO_REPORT_BUCKET_NAME"
        }
    }
}
